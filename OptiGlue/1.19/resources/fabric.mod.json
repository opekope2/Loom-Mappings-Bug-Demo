{
  "schemaVersion": 1,
  "id": "optiglue",
  "version": "${version}+1.19",
  "name": "OptiGlue",
  "description": "Minecraft glue library for OptiGUI",
  "authors": [
    "opekope2"
  ],
  "contact": {
    "homepage": "https://github.com/opekope2/OptiGUI",
    "sources": "https://github.com/opekope2/OptiGUI",
    "issues": "https://github.com/opekope2/OptiGUI/issues"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optiglue.InitializerKt::initialize"
      }
    ]
  },
  "mixins": [
    "optiglue.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=1.19 <1.19.3",
    "java": ">=${java}",
    "optigui": "=${version}"
  }
}
